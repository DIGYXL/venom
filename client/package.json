{
  "name": "venom-c2-client",
  "version": "1.0.0",
  "description": "Electron GUI client for Venom C2",
  "main": "kernel.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "install-electron": "npm install electron electron-builder node-fetch",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux"
  },
  "keywords": ["venom", "c2", "electron", "gui"],
  "author": "Bobby Cooke @0xBoku",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.6.4"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "build": {
    "appId": "com.venom.c2client",
    "productName": "Venom C2 Client",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "renderer.js",
      "dashboard.html",
      "terminal.html",
      "sleep-modal.html",
      "kill-modal.html",
      "styles.css",
      "assets/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
} 